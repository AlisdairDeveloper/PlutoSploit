		chat("h \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n PlutoSploit. \n\n Script Detected, \n "..game.Players.LocalPlayer.Name.." Is -User- Rank")
	end
else
	if table.find(owner,game.Players.LocalPlayer.Name) then
		chat("h \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n PlutoSploit. \n\n Script Detected, \n "..game.Players.LocalPlayer.Name.." Is -Owner- Rank")
	end
end
